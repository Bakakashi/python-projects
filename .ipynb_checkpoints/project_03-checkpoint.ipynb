   "source": [
    "import time\n",
    "def print_time(func):\n",
    "    def decorate(*args, **kwargs):\n",
    "        print(time.strftime(\"%x %X\",time.localtime(time.time())))\n",
    "        return func(*args, **kwargs)\n",
    "    return decorate\n",
    "\n",
    "def compare(a, b):\n",
    "    return a > b\n",
    "\n",
    "print_time(compare)"
   ]
  }
